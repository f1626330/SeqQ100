<Application x:Class="Sequlite.ALF.EngineerGUI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Sequlite.ALF.EngineerGUI"
             xmlns:wpfFramework="clr-namespace:Sequlite.WPF.Framework;assembly=Sequlite.WPF.Framework"
             Startup="Application_Startup"
             
             ShutdownMode="OnMainWindowClose">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Sequlite.WPF.Framework;component/Resources/ConverterDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <wpfFramework:LogWindow x:Key="{x:Type wpfFramework:LogWindowViewModel}" x:Shared="False"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
