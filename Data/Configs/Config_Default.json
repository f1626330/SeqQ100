{ //DEFAULT CONFIG FILE. Modify this file for each instrument
  "Version": 3, //configuration file internal version

  "LoggerConfig": {
	//Optional:NONE,NORMAL,DEBUG,TEST,STARTUP,OLAERROR,OLAWARNING,BENCHMARK
    "FilterOutFlags": "NONE", //set to NONE to log everything
    "OLAFilterOutFlags": "DEBUG" //set to NONE to log everything
  },

  "SimulationConfig": {
    "IsSimulation": false,//true:switch to simulation mode
    "IsMachineV2": true
  },

  "ProcessorAffinity": "0x3FF", //GUI application affinity, set to 0 if don't want to change affinity
  "MaxMemoryUsage_GB": 20, // How much memory (GB) is needed for anything besides OLA?
  "OLAMinimumTotalThreadCount": 118, // Special setting to increase the number of threads allocated to OLA on Windows 10 systems with >64 logical cores
  "EnableSaveAFImage": false,//Special setting to save autofocus images

  "RecipeRunConfig": {
    //MYDOC or PROGRAMDATA, or otherwise  must be a full use specified path such as "C:\\temp" 
    //MYDOC =  windows my document location
    //PROGRAMDATA = windows programData location
    "ImagingBaseDirSelection": "D:\\",

    //From ImagingBaseDirSelection, recipe run logging files go to the following subdir 
    "RecipeRunLogSubDir": "\\Sequlite\\ALF\\Recipe\\Recipelogs",

    //From ImagingBaseDirSelection, acquired images are saved to subdir "ExperimentName\\SequenceReadName\\Data"
    "AcquiredImageSubDir": "\\Sequlite\\ALF\\Recipe\\Images",

    // back up acquired images to its subdir "ExperimentName\\SequenceReadName\\Data"
    "AcquiredImageBackupLocation": "\\\\SequLiteNAS2\\Experiments",
    "DataBackupOptions": "ImageBackup", // available settings: "NoBackup", "ImageBackup", "OLABackup", or "ImageBackup,OLABackup"
    "UseSubFolderForDataBackup": true, // an Instrument sub-folder will be appended
    "AnalysisTaskLocation": "\\\\SequLiteNAS2\\Tasks", // location for creating a task for downstream analysis of OLA results

    "MaximumReadlength": 75,//obsolete
    "OLADataSizeMBPerTile": 1024,//OLA data size pre tile in MB for disk space requirement    
    "SaveSimulationImages": false,//save image in simulation mode

    //location for OLA executables and configuration files such as run.json (original named run.sh) 
    "OLADir": "C:\\bin",

    //OLASimulationImageBaseDir must be a full path such as "C:\\OLA\\Images" or an empty string.
    //If a full path is given, all images must be under its subdir "SequenceReadName\\Data";
    //If an empty string is set, the default imaging location will be determined 
    //by the above settings ImagingBaseDirSelection and AcquiredImageSubDir
    //i.e. ImagingBaseDirSelection + AcquiredImageSubDir + "\\ExperimentName\\SequenceReadName\\Data"
    "OLASimulationImageBaseDir": "E:\\Sequlite\\ALF\\Recipe\\Images\\b281x_210928-115910",
    "OLAProcessSimulationImages": false,//OLA in simulation mode
    "UsingTPL": true, // using Task parallel library to run OLA
    "UsingTPLForExtractIntensitiesByCell": true,//using task parallel to ExtractIntensities
    "OLASingleExtractMultipleImagesByCell": true,//obsolete
    "OLASingleExtractMultipleImagesByCellWithJC": false, // If true, ExtractInt will join cycles by itself
    "OLAUseJoinCycles": false, // cycles will be joined before each BaseCall execution
    "OLAJoinCyclesAtRunEnd": true, // Cycles will be joined after all cycles are processed. Not needed for BaseCall, but is useful for downstream analysis. Cannot be done if OLABaseCallOnlyPFClusters is true, because all bli-s must have the same number of clusters.
    "OLAMinimumCyclesToCreateTemplates": 4, // Use this # of cycles making sure OLAMinimumCyclesToCallBases is at least this big. 0: the software will select everything automatically.
    "OLAMinimumCyclesToCallBases": 5,//The starting point to call bases
    "OLABaseCallEveryNthCycle": 5, // Call bases, starting with OLAMinimumCyclesToCallBases cycle, every OLABaseCallEveryNthCycle cycles or on the last current cycle - whichever is larger
    "OLASlidingWindowMain": true, // Use sliding window for Read1 and Read2
    "OLASlidingWindowIndex": true, // Use sliding window for Index1 and Index2
    "OLASlidingWindowOverlapCycles": 2, // Start the calling window from the cycle equal to the last cycle minus the overlap
    "OLAMinimumOutput": true,//Minimum basecall output files
    "OLAUseDLL": false,//Unimplemented functionality
    "OLAIndexCLR": false, // If true, the proc-int-clr.fastq file of Read1 will be indexed. If false, only proc-int-cpf.fastq file of Read1 will be indexed.
    "OLAUpdateCUIOnEveryTileBaseCall": true, // If false, CUI will be updated only when all tiles are processed for the current range of cycles
    "OLACatchHardwareExceptionsInCpp": false,//config algorithm parameter -y for catching hardware exceptions
    "OLABackupOffline": true,//obsolete
    "OLABaseCallOnlyPFClusters": true,//call pass filter clusters

    // Sparse mapping options to be used with the Sliding Window, for all base calling ranges:
    // 0: do not use sparse mapping, i.e. always load all cycles up to the current. Ignore OLASparseMappingCycles.
    // 1: load the minimum number of cycles required by the -x BaseCall parameter. Ignore OLASparseMappingCycles.
    // 2: load a constant number of cycles equal to the OLASparseMappingCycles.
    // 3: load all cycles for as long as PF clusters are not settled, then keep loading a constant number of cycles 
    // equal to the maximum number of cycles loaded during PF cluster search. Ignore OLASparseMappingCycles.
    // 4: same as 3, but, after PF clusters are settled, load a constant number of cycles equal to the OLASparseMappingCycles.
    // NOTE: 
    // (1) The sparse mapping configuration is used for setting the 11th phasing parameter (-g) for BaseCall
    // (2) If OLABaseCallOnlyPFClusters is true, OLASparseMappingOption must be 1.
    "OLASparseMappingOption": 1,
    "OLASparseMappingCycles": 0,

    // Y-ngrams can be used only if the parameter file contains a YG entry.
    // If y-ngrams are used, they will always be used on the last range of cycles
    // If OLAFirstCycleToUseYngrams is <= 0, y-ngrams will be used only on the last range of cycles
    // If OLAFirstCycleToUseYngrams is > 0, y-ngrams will be first used on the first range of cycles, which includes the specified cycle
    // It is better to set OLAFirstCycleToUseYngrams to at least 30 
    "OLAFirstCycleToUseYngrams": -1,

    // OLAUseYngramsWithStepCycles=N means y-ngrams will be used on all ranges starting with the first range where y-ngrams are used, then going with step N cycles, and ending with the last range of cycles
    // If OLAUseYngramsWithStepCycles is <=0, y-ngrams will be used only on 2 ranges: OLAFirstCycleToUseYngrams and the last range
    // If OLAUseYngramsWithStepCycles > 0, make this a multiple of the sliding window step, i.e. OLABaseCallEveryNthCycle
    "OLAUseYngramsWithStepCycles": -1,

	//config algorithm parameter -x setting
    "OLAOutOfRangeBaseCallAllowed": true,
    "OLAUseScoresFromPreviousRangeWhenDefiningPFClusters": true,
    "OLASmoothBCQC": true,
    "OLASmoothBCQCIncludeOutOfRange": true,

    "OLAParams_Main_nonHG":  "p_b165x2_no_r2_D12g8_noQM",//obsolete
    "OLAParams_Main_HG":  "p_b165x2_YG_r2_D8g0_noQM",//Specifies the algorithm parameter file for Read
    "OLAParams_Index": "p_b165x2_idx_r2_D16"//Specifies the algorithm parameter file for Index
